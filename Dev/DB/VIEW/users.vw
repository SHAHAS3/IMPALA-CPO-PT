CREATE OR REPLACE FORCE VIEW USERS AS
SELECT "USER_ID",
    "NAME",
    "LOGIN",
    "EMAIL",
    "USER_TYPE",
    "IS_BLINDED",
    "IS_APPL_USER",
    "TELEPHONE",
    "FAX",
    "PWD_CHANGE_DATE",
    "DEF_TEXT_TOOLBAR",
    "AUTO_QUERY",
    "CREATED_BY",
    "DATE_CREATED",
    "MODIFIED_BY",
    "DATE_MODIFIED",
    "VALID_TILL_DATE",
    "FIRST_LOGON_PERIOD",
    "FIRST_LOGON",
    "IS_IVRS_ADMIN",
    "IS_ADMIN_USER",
    "COMMENTS",
    "PASSWORD",
    "LOCK_ATTEMPT",
    "LOCK_DATE",
    "ACCOUNT_STATUS",
    "ADDRESS"
